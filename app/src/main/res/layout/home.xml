<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <RelativeLayout


        android:background="@drawable/template"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <EditText
            android:id="@+id/et_search"
            android:layout_width="362dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="22dp"
            android:layout_marginTop="45dp"
            android:layout_marginEnd="27dp"
            android:drawableEnd="@drawable/ic_search"
            android:inputType="text" />

        <TextView
            android:id="@+id/tv_berlangsung"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/et_search"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="35dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="42dp"
            android:layout_marginEnd="220dp"
            android:text="@string/sedang_berlangsung"
            android:textColor="#E81212">

        </TextView>

        <TextView
            android:id="@+id/tv_pemilihangub"
            android:layout_width="139dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_berlangsung"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="160dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="94dp"
            android:text="@string/pemilihan_gubernur"
            android:textColor="#000000"
            android:textSize="14dp"
            android:textStyle="bold">

        </TextView>

        <TextView
            android:id="@+id/tv_isipemgub"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_pemilihangub"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="160dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="41dp"
            android:text="@string/pemilihan_gubernur_akan_berlangsung_hingga_21_april_2022_pilihlah_sesuai_dengan_visi_misi_yang_kamu_inginkan"
            android:textColor="#000000"
            android:textSize="11dp">

        </TextView>

        <ImageButton
            android:id="@+id/btn_tdk1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_isipemgub"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="192dp"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="56dp"
            android:background="@drawable/btn_tidak">

        </ImageButton>

        <TextView
            android:id="@+id/tv_tersedia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_isipemgub"
            android:layout_alignParentStart="true"
