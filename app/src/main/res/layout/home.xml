        </TextView>

        <TextView
            android:id="@+id/tv_pemilihanwal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="167dp"
            android:layout_marginEnd="103dp"
            android:layout_marginTop="15dp"
            android:layout_below="@id/tv_segera"
            android:text="@string/pemilihan_walikota"
            android:textColor="#000000"
            android:textSize="14dp"
            android:textStyle="bold">

        </TextView>

        <TextView
            android:id="@+id/tv_isipemwal"
            android:layout_width="194dp"
            android:layout_height="43dp"
            android:layout_below="@id/tv_pemilihanwal"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="167dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="33dp"
            android:text="@string/pemilihan_walikota_akan_berlangsung_hingga_21_mei_2022_pilihlah_sesuai_dengan_visi_misi_yang_kamu_inginkan"
